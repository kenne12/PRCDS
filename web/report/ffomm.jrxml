	<queryString>
		<![CDATA[SELECT
  public.pilier.idpilier,
  public.pilier.nom_fr,
  public.ffom_region.idffom_region,
  public.ffom_region.force,
  public.ffom_region.faiblesse,
  public.ffom_region.opportunite,
  public.ffom_region.menace

FROM
  public.region
  INNER JOIN public.ffom_region ON (public.region.idregion = public.ffom_region.idregion)
  INNER JOIN public.pilier ON (public.ffom_region.idpilier = public.pilier.idpilier)
WHERE
  public.pilier.idpilier = 4 AND
  public.region.idregion = $P{idregion}]]>
