	<queryString>
		<![CDATA[SELECT
  parametre.idparametre,
  public.annee.idannee,
  public.annee.nom AS annees,
  public.axe.idaxe,
  public.axe.code AS codeaxes,
  public.axe.nom_fr AS axes,
  public.sousaxe.idsousaxe,
  public.sousaxe.code AS codesouxaxes,
  public.sousaxe.nom_fr AS souxaxes,
 sum( public.activite_region.coutglobal) as montant
FROM
  public.axe
  INNER JOIN public.sousaxe ON (public.axe.idaxe = public.sousaxe.idaxe)
  INNER JOIN public.categorieintervention ON (public.sousaxe.idsousaxe = public.categorieintervention.idsousaxe)
  INNER JOIN public.interventionpnds ON (public.interventionpnds.idcategorieintervention = public.categorieintervention.idcategorieintervention)
  INNER JOIN public.indicateur ON (public.interventionpnds.idinterventionpnds = public.indicateur.idinterventionpnds)
  INNER JOIN public.indicateur_region ON (public.indicateur.idindicateur = public.indicateur_region.idindicateur)
  INNER JOIN public.probleme_region ON (public.indicateur_region.idindicateur_region = public.probleme_region.idindicateur_region)
  INNER JOIN public.activite_region ON (public.probleme_region.idprobleme_region = public.activite_region.idprobleme_region)
  AND (public.interventionpnds.idinterventionpnds = public.activite_region.idintervention)
  INNER JOIN public.sourcefinancement ON (public.sourcefinancement.idsourcefi = public.activite_region.idsourcefi)
  INNER JOIN public.chronogramme_region ON (public.activite_region.idactivite_region = public.chronogramme_region.idactivite_region)
  INNER JOIN public.annee ON (public.annee.idannee = public.chronogramme_region.idannee)
  INNER JOIN public.region ON (public.indicateur_region.idregion = public.region.idregion),
  parametre
WHERE
  public.annee.chronogramme = 'true' AND
  interventionpnds.region = 'true' AND
  public.region.idregion = $P{idregion}
GROUP BY
  parametre.idparametre,
  public.annee.idannee,
  public.annee.nom,
  public.axe.idaxe,
  public.axe.code,
  public.axe.nom_fr,
  public.sousaxe.idsousaxe,
  public.sousaxe.code,
  public.sousaxe.nom_fr
ORDER BY
  public.axe.code]]>
	</queryString>
