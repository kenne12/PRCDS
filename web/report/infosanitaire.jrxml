	<queryString>
		<![CDATA[SELECT
  public.rubriqueinfosanitaire.idrubriqueinfosanitaire,
  public.rubriqueinfosanitaire.nom_fr AS rubriques,
  public.structure.idstructure,
  public.structure.nom_fr AS structures,
  public.typestructure.idtypestructure,
  public.typestructure.nom_fr AS typestructures,
  public.informationsanitaire_region.idinformationsanitaire_region,
  public.informationsanitaire_region.valeur,
  public.region.idregion,
  public.commentaire_region.commentaire
FROM
  public.structure
  INNER JOIN public.typestructure ON (public.structure.idtypestructure = public.typestructure.idtypestructure)
  INNER JOIN public.informationsanitaire_region ON (public.structure.idstructure = public.informationsanitaire_region.idstructure)
  INNER JOIN public.rubriqueinfosanitaire ON (public.rubriqueinfosanitaire.idrubriqueinfosanitaire = public.informationsanitaire_region.idrubriqueinfosanitaire)
  INNER JOIN public.region ON (public.informationsanitaire_region.idregion = public.region.idregion)
  INNER JOIN public.commentaire_region ON (public.region.idregion = public.commentaire_region.idregion)
WHERE
  public.commentaire_region.numerotab = 28 and public.region.idregion = $P{idregion}
ORDER BY
  public.typestructure.nom_fr,
  public.structure.nom_fr,
  public.rubriqueinfosanitaire.nom_fr]]>
	</queryString>
